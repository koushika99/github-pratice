            }
        }
        stage("Stage 2"){
            steps{
                echo " This is stage 2"
            }
        }
        stage("Stage 3"){
            steps{
                echo " This is stage 3"
            }
        }
        stage("Stage 4"){
            steps{
                echo "Chnaged to scm for 2 mins"
            }
        }
        stage("Stage 5"){
            steps{
                echo "Changed to scm for H/2"
            }
        }
